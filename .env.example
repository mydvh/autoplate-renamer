# Environment variables for Docker Compose
# Copy this file to .env and fill in your values

# Database Configuration (Docker internal network)
DB_TYPE=postgres
DB_HOST=postgres
DB_PORT=5432
DB_USER=autoplate_user
DB_PASSWORD=autoplate_password_2024
DB_NAME=autoplate_renamer

# JWT Secret (Change this in production!)
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Gemini API Key (Required for image analysis)
GEMINI_API_KEY=your-gemini-api-key-here

# Frontend URL for CORS
FRONTEND_URL=https://anhxe.dangkiemxe.net

# Domain Configuration
DOMAIN=anhxe.dangkiemxe.net
EMAIL=admin@anhxe.dangkiemxe.net

# Node Environment
NODE_ENV=production
